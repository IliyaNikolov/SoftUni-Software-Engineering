package ArraysExercise;

import java.util.Scanner;

public class Train_01 {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        int countWagons = Integer.parseInt(scanner.nextLine());
        int[] peopleInWagon = new int[countWagons];

        int sum = 0;
        for (int index = 0; index < countWagons; index++) {
            peopleInWagon[index] = Integer.parseInt(scanner.nextLine());
            sum += peopleInWagon[index];
            System.out.print(peopleInWagon[index] + " ");
        }
        System.out.printf("%n%d", sum);
    }
}
