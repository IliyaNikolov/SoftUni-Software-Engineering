package DataTypesExercise;

import java.util.Scanner;

public class TriplesOfLatinLetters_06 {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        int n = Integer.parseInt(scanner.nextLine());

        for (int letter1 = 0; letter1 < n; letter1++) {
            char firstChar = (char) ('a' + letter1);
            for (int letter2 = 0; letter2 < n; letter2++) {
                char secondChar = (char) ('a' + letter2);
                for (int letter3 = 0; letter3 < n; letter3++) {
                    char thirdChar = (char) ('a' + letter3);
                    System.out.printf("%c%c%c%n", firstChar, secondChar, thirdChar);
                }
            }
        }
    }
}
